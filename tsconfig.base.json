{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@my-nx-monorepo/question-randomizer-auth-feature": [
        "libs/question-randomizer/auth/feature/src/index.ts"
      ],
      "@my-nx-monorepo/question-randomizer-auth-shell": [
        "libs/question-randomizer/auth/shell/src/index.ts"
      ],
      "@my-nx-monorepo/question-randomizer-auth-util": [
        "libs/question-randomizer/auth/util/src/index.ts"
      ],
      "@my-nx-monorepo/question-randomizer-data-access": [
        "libs/question-randomizer/auth/data-access/src/index.ts"
      ],
      "@my-nx-monorepo/shared-ui": ["libs/shared/ui/shared-ui/src/index.ts"],
      "@my-nx-monorepo/shared-util": ["libs/shared/util/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
